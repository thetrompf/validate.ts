{
    "author": "Brian Kejlberg <briankchristensen@gmail.com>",
    "dependencies": {
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jest": "^23.3.9",
        "@types/node": "^10.12.10",
        "eslint": "^5.9.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "jest": "^23.6.0",
        "prettier": "^1.15.2",
        "serve": "^10.1.1",
        "ts-jest": "^23.10.5",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "uglify-js": "^3.4.9"
    },
    "license": "BSD-3-Clause",
    "main": "lib/index.js",
    "module": "lib2015/validate.js",
    "name": "validate.ts",
    "repository": "https://git.input-output.dk/bkc/validate.ts",
    "scripts": {
        "_build:dist-amd": "tsc -p tsconfig.dist.amd.json",
        "_build:dist-system": "tsc -p tsconfig.dist.system.json",
        "build:dist-amd": "yarn clean:dist-amd && yarn _build:dist-amd && yarn compress:dist-amd",
        "build:dist-system": "yarn clean:dist-system && yarn _build:dist-system && yarn compress:dist-system",
        "build:dist": "yarn build:dist-amd && yarn build:dist-system",
        "build:lib-commonjs": "yarn clean:lib-commonjs && tsc -p tsconfig.lib.commonjs.json",
        "build:lib-es2015": "yarn clean:lib-es2015 && tsc -p tsconfig.lib.es2015.json",
        "build:source": "yarn clean:build && tsc -p tsconfig.json",
        "build": "yarn build:dist-amd && yarn build:dist-system && yarn build:lib-commonjs && yarn build:lib-es2015",
        "clean:build": "rm -Rf build/",
        "clean:coverage": "rm -Rf coverage/",
        "clean:dist-amd": "rm -Rf dist/amd/",
        "clean:dist-system": "rm -Rf dist/system/",
        "clean:dist": "rm -Rf dist",
        "clean:lib-commonjs": "rm -Rf lib/",
        "clean:lib-es2015": "rm -Rf lib2015/",
        "clean:lib": "yarn run clean:lib-commonjs && yarn run clean:lib-es2015",
        "clean:types": "rm -Rf types",
        "clean": "yarn clean:build && yarn clean:coverage && yarn clean:dist && yarn clean:lib && yarn clean:types",
        "compress:dist-amd": "uglifyjs --compress warnings=false --mangle --screw-ie8 --acorn --output dist/amd/validate.min.js -- dist/amd/validate.js",
        "compress:dist-system": "uglifyjs --compress warnings=false --mangle --screw-ie8 --acorn --output dist/system/validate.min.js -- dist/system/validate.js",
        "coverage": "yarn clean:coverage && jest --coverage",
        "docker:build": "docker build -t bkc/validate.ts . && docker run --rm -itv .:/artifacts bkc/validate.ts",
        "fmt": "prettier --write --print-width 120 --tab-width 4 --single-quote --trailing-comma all --parser typescript 'src/**/*.ts'",
        "lint": "tslint .",
        "prepublishOnly": "test -n \"$NPM_TOKEN\" || (>&2 echo 'Only publishing through CI is supported' && exit 1)",
        "serve:examples": "yarn build:dist-amd && serve --ignore='.vscode/,coverage/,dist/system/,node_modules/,src/,.eslintrc.json,.gitignore,.gitlab-ci.yml,.npmignore,CHANGELOG.md,LICENSE,README.md,npm-shrinkwrap.json,package.json,tsconfig.dist.amd.json,tsconfig.dist.system.json,tsconfig.json,tsconfig.lib.commonjs.json,tsconfig.lib.es2015.json,tsconfig.test.json,tslint.json,yarn.lock'",
        "test": "jest"
    },
    "types": "types/index.d.ts",
    "version": "2.1.1"
}
