{
    "compilerOptions": {
        "importHelpers": true,
        "module": "commonjs",
        "noEmitHelpers": true,
        "noImplicitAny": true,
        "outDir": "lib",
        "rootDir": "src",
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2015",
        "lib": [
            "es2015"
        ]
    },
    "exclude": [
        "node_modules",
        "src/**/__tests__/*"
    ]
}
