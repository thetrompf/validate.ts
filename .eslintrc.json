{
    "extends": "airbnb",
    "rules": {
        "comma-dangle": [
            2,
            {
                "arrays": "always-multiline",
                "objects": "always-multiline",
                "imports": "always-multiline",
                "exports": "always-multiline",
                "functions": "ignore"
            }
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-underscore-dangle": [
            0
        ],
        "quote-props": [
            "error",
            "consistent-as-needed",
            {
                "keywords": true,
                "unnecessary": false
            }
        ]
    }
}
